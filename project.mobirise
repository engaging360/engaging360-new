          "_name": "ampEvents14",
          "_customHTML": "<section class=\"ampEvents14\" group=\"Events\" plugins=\"amp-main\">\n    <mbr-parameters>\n        <input type=\"range\" inline=\"\" title=\"Top\" name=\"paddingTop\" min=\"0\" max=\"10\" step=\"1\" value=\"5\">\n        <input type=\"range\" inline=\"\" title=\"Bottom\" name=\"paddingBottom\" min=\"0\" max=\"10\" step=\"1\" value=\"5\">\n        <input type=\"checkbox\" title=\"Full Width\" name=\"fullWidth\">\n            <header>Cards</header>\n        <select title=\"CardsAmount\" name=\"cardsAmount\">\n            <option value=\"1\">1</option>\n            <option value=\"2\">2</option>\n            <option value=\"3\" selected=\"\">3</option>\n            <option value=\"4\">4</option>\n        </select>\n        <input type=\"color\" title=\"Card Background\" name=\"cardBG\" value=\"#ffffff\" condition=\"transparentCard\">\n        <header>Cards Text</header>\n        <input type=\"checkbox\" title=\"Show Title\" name=\"showCardsTitle\" checked=\"\">\n        <input type=\"checkbox\" title=\"Show Text\" name=\"showCardsText\" checked=\"\">\n        <input type=\"checkbox\" title=\"Show Post\" name=\"showPost\" checked=\"\">\n        <header>Cards Image</header>\n        <input type=\"checkbox\" title=\"Show Image\" name=\"showImg\" checked=\"\">\n        <input type=\"range\" inline=\"\" title=\"Image Height\" name=\"imgHeight\" min=\"200\" max=\"450\" step=\"10\" value=\"270\" condition=\"showImg\">\n        <input type=\"checkbox\" title=\"Image Effect\" name=\"imgEffect\" condition=\"showImg\" checked=\"\">\n        <input type=\"color\" title=\"Overlay Color\" name=\"bgImage\" value=\"#ffffff\" condition=\"showImg &amp;&amp; imgEffect\">\n        <input type=\"range\" inline=\"\" title=\"Image Overlay Opacity\" name=\"imgOverlayOpacity\" min=\"0\" max=\"0.9\" step=\"0.05\" value=\"0.25\" condition=\"showImg &amp;&amp; imgEffect\">\n        <header>Cards Bottom</header>\n        <input type=\"checkbox\" title=\"Show Bottom\" na